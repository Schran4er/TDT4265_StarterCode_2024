## Font size ##
font.family        : serif
font.serif         : Times New Roman
text.usetex        : true
font.size          : 11

text.latex.preamble : \usepackage{amsmath}\usepackage{times}\usepackage[version=4]{mhchem}

lines.markersize : 3
lines.linewidth : 1.25

